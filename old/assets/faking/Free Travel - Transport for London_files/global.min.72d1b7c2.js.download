                    place.name = match.commonName;
                    place.placeId = place.id;
                    place.modes = match.placeType;
                    place.lat = match.lat;
                    place.lon = match.lon;
                }
                break;

              case "placesExtra":
                match = response.matches.find(function(match) {
                    if (placeName === match.name) {
                        return match;
                    }
                });
                place.id = placeId;
                var placeLatLon = favUtils.getGeoLocation(placeId);
