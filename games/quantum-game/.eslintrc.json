{
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
  },
  "plugins": ["lodash"],
  "extends": ["eslint:recommended", "plugin:lodash/recommended"],
  "rules": {
    "comma-dangle": [1, "always-multiline"],
    "no-var": 2,
    "quotes": [1, "single"],
    "no-unused-vars": 1,
    "lodash/prefer-lodash-method": 0
  },
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true
  }
}
